# basic query : name, pic according to dbr id
select ?name, ?depiction
where {
dbr:Pablo_Picasso rdfs:label ?name.
dbr:Pablo_Picasso foaf:depiction ?depiction.
Filter(lang(?name) = "fr").
} 

#other attributes
dbo:deathDate
dbo:deathPlace
dbo:birthDate
dbo:birthPlace
dbo:movement
dbp:works


#get artists from movement's dbr id
select ?s 
where{
?s dbo:movement dbr:Surrealism
}
LIMIT 10

#get pictures of painter's works:
select distinct ?p ?depict where {
?p dct:subject ?sub ;dbo:author dbr:Pablo_Picasso ;foaf:depiction ?depict
FILTER(CONTAINS(str(?sub),"Painting"))
}

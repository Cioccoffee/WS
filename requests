# basic query : name, pic according to dbr id
select ?name, ?depiction
where {
dbr:Pablo_Picasso rdfs:label ?name.
dbr:Pablo_Picasso foaf:depiction ?depiction.
Filter(lang(?name) = "fr").
} 

#other attributes
dbo:deathDate
dbo:deathPlace
dbo:birthDate
dbo:birthPlace
dbo:movement
dbp:works

dbo:abstract (vue détaillée)
rdfs:comment (voir avec abstract)

dbp:nationality
dct:description (shorter than abstract)
foaf:surname
dbo:author
foaf:depiction

#complex request

select distinct ?name, ?depiction, ?deathDate, ?deathPlace, ?birthDate, ?movement
where {
dbr:Pablo_Picasso rdfs:label ?name;
foaf:depiction ?depiction;
dbo:deathDate ?deathDate;
dbo:deathPlace ?deathPlace;
dbo:birthDate ?birthDate;
dbo:birthPlace ?birthPlace;
dbo:movement ?movement.
Filter(lang(?name) = "fr").
}
LIMIT 1


#get artists from movement's dbr id
select ?s 
where{
?s dbo:movement dbr:Surrealism
}
LIMIT 10

#get pictures of painter's works:
select distinct ?p ?depict where {
?p dct:subject ?sub ;dbo:author dbr:Pablo_Picasso ;foaf:depiction ?depict
FILTER(CONTAINS(str(?sub),"Painting"))
}
